<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />
    <constant name="struts.allowed.action.names" value="[a-zA-Z0-9._!/\-]*"></constant>
    <package name="StudentSSH" namespace="/" extends="struts-default">
        
        <action name="loginaction" class="Action.LoginAction">
            <result name="good"> jsp/HelloWorld.jsp </result>
            <result name="bad"> jsp/error.jsp </result>
            
        </action>
    </package>

    <!-- Add packages here -->

</struts>
